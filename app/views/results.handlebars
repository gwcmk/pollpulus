<div class="question hidden">
	{{question.question}}
</div>

{{#each answers}}
<div class="answer-data hidden">
	<div class="answer">
		{{answer}}
	</div>
	<div class="votes">
		{{votes}}
	</div>
</div>
{{/each}}

<a href="/questions" id="back-to-questions" class="btn btn-default btn-green">Back to other questions</a>

<div id="pie" style="height: 80vh width: 50vw; margin: 10vh auto"></div>
